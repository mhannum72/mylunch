                            }
                            else {

                                lastGrid.remove();
                                nextPrevPageLinks(parseInt(response.nextpage, 10), page_globals.prevPage);

                            }
                        }

                        // Destroy carousel
                        elm.destroy();

                        // Fade out popup
                        popup.fadeOut(0);

